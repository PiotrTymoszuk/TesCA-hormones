issn = {14795876},
journal = {Journal of Translational Medicine},
keywords = {Cancer immunotherapy,Cancer testis antigen,Cancer vaccine,Immune checkpoint inhibitors,NY-ESO-1,Tumor microenvironment},
month = {mar},
